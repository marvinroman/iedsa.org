{
  "name": "nuxt3-with-vuetify3-example-template",
  "version": "1.1.0",
  "description": "Nuxt3 with vuetify3 example template.",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass,html,vue}",
    "lint:style:fix": "stylelint **/*.{css,scss,sass,html,vue} --fix",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "deploy": "gh-pages --dotfiles -d .output/public"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@mdi/font": "7.0.96",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "commitizen": "^4.2.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "inquirer": "8",
    "lint-staged": "^13.0.3",
    "npm-check": "^6.0.1",
    "nuxt": "^3.12.4",
    "postcss": "^8.4.31",
    "postcss-html": "^1.3.0",
    "prettier": "^3.5.3",
    "sass": "^1.55.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^4.8.4",
    "vuetify": "^3.1.0"
  },
  "dependencies": {
    "@nuxt/content": "^2.6.0",
    "gh-pages": "^5.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
